@CHARSET "GB18030";

html {
	font-family: sans-serif;
	-webkit-text-size-adjust: 100%;
	-ms-text-size-adjust: 100%;
}

body {
	width:1000px;
	margin: 0 auto;
	height:700px;
}
.content{
	width:1000px;
	height:700px;
	background-image: url("../../images/back-image.gif");
}
.loginborder{
	width:500px;
	height:300px;
	border:1px solid #ccc;
	margin:0 auto;
	position: absolute;
	top:200px;
	left:30%;
	border-radius: 15px;
	background-color:#C5DFF8;
}

.center{
	width:400px;
	height:auto;
	text-align: center;
	line-height:70px;
	margin:0 auto;
	position:absolute;
	left:11%;
	margin-top:20px;
}
#userName{
	
	height:25px;
	width:250px;
	line-height: 25px;
}
/*border-radius: 20px;*/
#psd{
	
	height:25px;
	width:250px;
}
.logins{
	border-radius:15px;
